#include <stdio.h> 

int main(int num){

    //тук задаваме стойност на нашата променлива
    num = 400;
    
    //правим маска с нули на четните позиции 
    int mask = 0xAAAAAAAA;
    
    //прилагаме побитов оператор AND
    num = num & mask;

    //принтираме новото число 
    printf("%d\n", num);

    return 0;
}
