#include <stdio.h>

int addDigit(int a, int b);

int main() { 

    //тук задавам стойност на двете събираеми
    int a = 6;
    int b = 8;

    addDigit(a,b);

    return 0;
}

int addDigit(int a, int b) {

    //създавам променлива за резултата
    int c;
    
    c = a + b;

    printf("%d\n", c);

    return c;
}
